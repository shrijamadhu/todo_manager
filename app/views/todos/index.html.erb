<article>
    <h1 class="Todo-start"> My Todo List</h1>
    <section class="Todo-start-section">
       <input class="Todo-text" type="text" placeholder="what's next?" />
       <input class="Todo-date" type="date" />
       <input class="Todo-add-button" type="button"  value="ADD"  />
    </section>
 <%= render partial: "todo-section",locals: {
   section_title: "Overdue",
   todos: Todo.overdue,
   show_due_date: true
   }%>
 <%= render partial: "todo-section",locals: {
   section_title: "Due Today",
   todos: Todo.due_today,
   show_due_date: false
   }%>
 <%= render partial: "todo-section",locals: {
   section_title: "Due Later",
   todos: Todo.due_later,
   show_due_date: true
   }%>

</article>
